- var title = 'Location Revenue Report';
html
  include head.jade
  body
    include header.jade
    .main-panel.container-fluid
        form#login-form(action='/login', method='POST')
            .form-group
              label(for='username') Username
              input.form-control(type='text', name='username', required='')
            .form-group
              label(for='password') Password
              input.form-control(type='password', name='password' required='')
            .form-group
              input.login-submit.form-control.btn.btn-default(type='submit')
    include footer.jade